-- Creating A Table

CREATE TABLE book_list (id INTEGER PRIMARY KEY, name TEXT, rating INTEGER);

INSERT INTO book_list VALUES (1, "The Bible", 10);
INSERT INTO book_list VALUES (2, "Harry Potter", 9.6);
INSERT INTO book_list VALUES (3, "Lord of the Rings", 8.3);
INSERT INTO book_list VALUES (4, "Rich Dad Poor Dad", 8.7);

Select *
From book_list
